{
    "name": "par/core",
    "description": "Core structures",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.2"
        }
    },
    "autoload": {
        "psr-4": {
            "PAR\\Core\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PAR\\Core\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.2"
    },
    "require-dev": {
        "par/coding-standard": "^1.0",
        "par/maker": "^1.0",
        "phpstan/phpstan": "^0.11.8",
        "phpunit/phpunit": "^8.1",
        "roave/security-advisories": "dev-master"
    }
}
